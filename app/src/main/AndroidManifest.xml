<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.khavronsky.unitedexercises"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <application
        android:name=".utils.di.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".presentation.exercise.exercise_performance.ExercisePerformActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.exercise.create_new_exercises.new_cardio_exercise.CardioExerciseEditorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.exercise.create_new_exercises.new_power_exercise.PowerExerciseEditorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name=".presentation.exercise.exercises_catalogs.ExerciseCatalogActivity"
            android:screenOrientation="portrait" />
        <activity android:name=".presentation.exercise.PowerExHelp"></activity>
    </application>

</manifest>